generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Equation {
  id    Int    @id @default(autoincrement())
  name  String @unique
}
model root {
  id    Int    @id @default(autoincrement())
  name  String @unique
  xl    Float
  xr    Float 
}
model linear {
  id    Int    @id @default(autoincrement())
  size Int 
  A   Json @unique
  B    Json
  x0    Json 
}
model inter {
  id    Int    @id @default(autoincrement())
  point Int 
  X   Json @unique
  Y    Json
  x0    Json 
}

